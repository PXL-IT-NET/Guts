    "@angular/forms": "^15.2.7",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@angular/platform-server": "^15.2.7",
    "@angular/router": "^15.2.7",
    "@ng-bootstrap/ng-bootstrap": "14.1.0",
    "angular-2-local-storage": "^3.0.2",
    "bootstrap": "5.2.3",
    "bootstrap-icons": "^1.10.3",
    "chart.js": "~4.2.1",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "moment": "^2.24.0",
    "ng-recaptcha": "11.0.0",
    "ng2-charts": "^4.1.1",
    "ngx-loading": "15.0.0",
