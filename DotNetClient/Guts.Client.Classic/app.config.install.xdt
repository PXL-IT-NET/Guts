<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings xdt:Transform="Insert" />
  <appSettings>
    <!--http://localhost.fiddler/Guts.Api/-->
    <add key="GutsApiUri" xdt:Transform="Remove" xdt:Locator="Match(key)" />
    <add key="GutsApiUri" value="https://guts-api.pxl.be/" xdt:Transform="Insert" />
    <add key="GutsWebUri" xdt:Transform="Remove" xdt:Locator="Match(key)" />
    <add key="GutsWebUri" value="https://guts-web.pxl.be/" xdt:Transform="Insert" />
  </appSettings>
  <appSettings xdt:Transform="Remove" xdt:Locator="XPath(/configuration/appSettings[2])" />
</configuration>